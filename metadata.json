{ "name": "eyeo-ansible",
  "version": "0.0.1",
  "author": "Mathias J. Hennig",
  "license": "GPL-3.0+",
  "summary": "Integrate Ansible with Puppet",
  "source": "https://github.com/mjhennig/puppet-ansible",
  "project_page": "https://forge.puppetlabs.com/eyeo/ansible",
  "issues_url": "https://github.com/mjhennig/puppet-ansible/issues",
  "tags": [
    "ansible",
    "puppet"
